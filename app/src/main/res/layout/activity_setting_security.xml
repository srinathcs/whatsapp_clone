    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.whatsapp.clone.widgets.settingSecurity.SecurityFirstWeb
        android:id="@+id/securityFirstWeb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp15" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_85" />

    <com.whatsapp.clone.widgets.settingSecurity.SecuritySecondWeb
        android:id="@+id/securitySecondWeb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
         />
</androidx.appcompat.widget.LinearLayoutCompat>